<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>Keypad_Editor</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\BackImage.png" />
    <None Remove="Resources\close_window.png" />
    <None Remove="Resources\Keypad.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\BackImage.png" />
    <Resource Include="Resources\close_window.png" />
    <Resource Include="Resources\Keypad.ico" />
  </ItemGroup>

</Project>
